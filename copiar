#include <stdio.h>
#include <stdlib.h>
#include "chip.h"

int main(){
    
    FILE *rom = fopen("./logo.ch8","rb");

    if(rom == NULL)
   {
      printf("Error!");   
      exit(1);             
   }

    fseek(rom,0,SEEK_END);
    size_t size = ftell(rom);
    fseek(rom,0,SEEK_SET);

    unsigned char buffer[size];

    fread(buffer,size,1,rom);
    fclose(rom);

    for(int i=0 ; i<size ; i++){
        printf("HEX : 0x%02X\n",buffer[i]);
    }

    return 0;
}